@{
	//var errorMessages =
	//ViewData["ErrorMessages"] as
	//System.Collections.Generic.IList<string>;

	//var errorMessages =
	//ViewData[index: Infrastructure.BasePageModel.ErrorMessagesKeyName] as
	//System.Collections.Generic.IList<string>;

	var errorMessages =
	TempData[key: Infrastructure.BasePageModel.ErrorMessagesKeyName] as
	System.Collections.Generic.IList<string>;

	if (errorMessages != null && errorMessages.Count != 0)
	{
		<div class="alert alert-danger pb-0" role="alert">
			<ul>
				@foreach (var message in errorMessages)
				{
					<li>@(message)</li>
				}
			</ul>
		</div>
	}

	var warningMessages =
	TempData[key: Infrastructure.BasePageModel.WarningMessagesKeyName] as
	System.Collections.Generic.IList<string>;

	if (warningMessages != null && warningMessages.Count != 0)
	{
		<div class="alert alert-warning pb-0" role="alert">
			<ul>
				@foreach (var message in warningMessages)
				{
					<li>@(message)</li>
				}
			</ul>
		</div>
	}

	var successMessages =
	TempData[key: Infrastructure.BasePageModel.SuccessMessagesKeyName] as
	System.Collections.Generic.IList<string>;

	if (successMessages != null && successMessages.Count != 0)
	{
		<div class="alert alert-success pb-0" role="alert">
			<ul>
				@foreach (var message in successMessages)
				{
					<li>@(message)</li>
				}
			</ul>
		</div>
	}
}
